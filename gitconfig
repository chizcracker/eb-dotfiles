[alias]
  co = checkout
  st = status
  ci = commit
  pu = pull --rebase
  ps = push
  br = branch
  l = log --graph --pretty=format:'%an: %s - %Cred%h%Creset %C(yellow)%d%Creset %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
[color]
  diff = auto
  status = auto
  branch = auto
[core]
  excludesfile = /Users/ebenoist/.gitignore
  editor = vim
  autocrlf = input
[apply]
  whitespace = nowarn
[web]
  browser = open
[include]
  path = ~/.gituser
